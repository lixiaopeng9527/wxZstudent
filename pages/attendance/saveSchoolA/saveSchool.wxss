  margin-top: 20rpx;
}



.fuzzy-query{
  position: absolute;
  top: 110rpx;
  z-index: 100;
  width:100%;
  left:0;
  
}

.fuzzy-query-list{
  overflow: hidden;
  background: #fff; 
  height: 600rpx;
 
}

.fuzzy-query-item{
  display: flex;
  flex-direction: row;
  align-items: center; 
  padding: 25rpx 0rpx;
  margin:  0rpx 20rpx;
  border-bottom: 2rpx solid #D8D8D8; 
  color: #6E6E6E;
}



.fuzzy-query-name{
  font-size:30rpx;
 
}

.fuzzy-query-content{
  flex-direction: column;  
  display: flex;
   margin-left:20rpx;
}

.fuzzy-query-desc{
  font-size: 24rpx;
color: #9D9D9D;
}

.save-school-query-layer{
  position: relative;
  flex-direction: row;
  align-items: center;
  display: flex;

  border-bottom: 2rpx solid #DEDEDE;
}

.save-school-query-complete{
  color: #00A7F7;
  font-size: 30rpx;
  position: absolute;
  right: 0;
  top: 0;
}

.save-school-name{
  padding-right : 30rpx;  
  border-bottom: 0  !important;
}

.save-school-label{
  /* font-weight: bold; */
