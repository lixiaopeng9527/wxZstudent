.save-school-form{
  margin-bottom: 40rpx;
  margin-top: 30rpx;
  overflow:visible;
  position: relative;
  z-index: 100;

}
.save-school-form-first-input{
  padding-top: 0;
}



.save-school-address{
  padding-top: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.company-title{
  color: #0081C4;
  font-size: 24rpx;
